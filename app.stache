<can-import from="~/components/loading/" />

{{#if(isAuthenticated)}}
  <can-import from="~/components/dashboard/">
    {{#if(isPending)}}
      <bitovian-pto-loading />
    {{else}}
      <bitovian-pto-dashboard apiInfo:from="apiInfo" />
    {{/if}}
  </can-import>
{{else}}
  <can-import from="~/components/authenticate/">
    {{#if(isPending)}}
      <bitovian-pto-loading />
    {{else}}
      <bitovian-pto-authenticate apiInfo:from="apiInfo" />
    {{/if}}
  </can-import>
{{/if}}