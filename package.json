{
  "name": "pto-tracker",
  "version": "0.1.0",
  "description": "Freshbooks-integrated PTO Tracker for Bitovi employees",
  "main": "app.js",
  "scripts": {
    "build": "steal-tools",
    "bundle": "steal-tools bundle --dev",
    "lint": "standard --fix 'components/**/*.js' 'models/**/*.js'",
    "postinstall": "npm run bundle",
    "start": "http-server -c-1"
  },
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.11.1",
    "standard": "^12.0.1",
    "steal-tools": "^2.0.6"
  },
  "dependencies": {
    "can": "^5.16.0",
    "steal": "^2.1.6",
    "steal-less": "^1.3.4",
    "steal-stache": "^4.1.2"
  },
  "steal": {
    "plugins": [
      "steal-less",
      "steal-stache"
    ]
  }
}
